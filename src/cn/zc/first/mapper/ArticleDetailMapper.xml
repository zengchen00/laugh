<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.zc.first.mapper.ArticleDetailMapper" >
	<insert id="insert" parameterType="articleDetail" >
		insert into F_ARTICLE_DETAIL(DESCRIPTION,ARTICLE_ID,IMG_URL,CREATE_DATE,MODIFY_DATE)
		 value(#{description},#{article.id},#{img_url},#{modifyDate},#{createDate})
	</insert>
</mapper>